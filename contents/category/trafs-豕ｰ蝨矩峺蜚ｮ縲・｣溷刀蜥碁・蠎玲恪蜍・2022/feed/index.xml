		<link>/contents/43071/</link>
					<comments>/contents/43071/#respond</comments>
		
		<dc:creator><![CDATA[user]]></dc:creator>
		<pubDate>Tue, 29 Nov 2022 08:07:22 +0000</pubDate>
				<category><![CDATA[movie]]></category>
