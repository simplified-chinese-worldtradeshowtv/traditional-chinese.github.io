		<link>/contents/41925/</link>
					<comments>/contents/41925/#respond</comments>
		
		<dc:creator><![CDATA[user]]></dc:creator>
		<pubDate>Fri, 09 Sep 2022 06:36:10 +0000</pubDate>
				<category><![CDATA[movie]]></category>
