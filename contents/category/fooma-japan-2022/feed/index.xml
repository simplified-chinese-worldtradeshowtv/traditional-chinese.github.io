		<link>/contents/40980/</link>
					<comments>/contents/40980/#respond</comments>
		
		<dc:creator><![CDATA[user]]></dc:creator>
		<pubDate>Fri, 17 Jun 2022 04:36:24 +0000</pubDate>
				<category><![CDATA[FOOMA JAPAN 2022]]></category>
		<category><![CDATA[movie]]></category>
		<category><![CDATA[日本]]></category>
