		<link>/contents/41216/</link>
					<comments>/contents/41216/#respond</comments>
		
		<dc:creator><![CDATA[user]]></dc:creator>
		<pubDate>Tue, 12 Jul 2022 11:06:57 +0000</pubDate>
				<category><![CDATA[2022 年日本活動週]]></category>
		<category><![CDATA[movie]]></category>
		<category><![CDATA[愛好/教育]]></category>
		<category><![CDATA[日本]]></category>
